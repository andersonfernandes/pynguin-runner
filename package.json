{
  "name": "pynguin-runner",
  "repository": {
    "type": "git",
    "url": "https://github.com/andersonfernandes/pynguin-runner.git"
  },
  "displayName": "Pynguin Runner",
  "description": "Generate unit tests with Pynguin through Visual Studio Code",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Pynguin",
      "properties": {
        "pynguin.version": {
          "type": "string",
          "default": "0.34.0",
          "enum": ["0.34.0", "0.33.0"]
        },
        "pynguin.maximumSearchTime": {
          "type": "string",
          "default": "600",
          "description": "Specifies the maximun amount of search time."
        }
      }
    },
    "commands": [
      {
        "command": "pynguin-runner.generateTests",
        "title": "Run Pynguin for the current module"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}
